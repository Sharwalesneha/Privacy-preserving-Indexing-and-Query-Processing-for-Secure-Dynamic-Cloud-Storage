<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ownerlogin</servlet-name>
        <servlet-class>com.app.action.ownerlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>com.app.action.upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sharedb</servlet-name>
        <servlet-class>com.app.action.sharedb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestAccept</servlet-name>
        <servlet-class>com.app.action.RequestAccept</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReqDB</servlet-name>
        <servlet-class>com.app.action.ReqDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ownerlogin</servlet-name>
        <url-pattern>/ownerlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sharedb</servlet-name>
        <url-pattern>/sharedb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestAccept</servlet-name>
        <url-pattern>/RequestAccept</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReqDB</servlet-name>
        <url-pattern>/ReqDB</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
